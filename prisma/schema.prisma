generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Posts {
  id           BigInt    @id @default(autoincrement())
  date_created DateTime? @default(dbgenerated("(now() AT TIME ZONE 'pst'::text)")) @db.Timestamptz(6)
  name         String?   @default("255") @db.VarChar
  email        String?
  phone        Decimal?  @db.Decimal
  address      String?
  city         String?
  state        String?
  zipcode      Decimal?  @db.Decimal
}

model Users {
  id           BigInt    @id @default(autoincrement())
  date_created DateTime? @default(dbgenerated("(now() AT TIME ZONE 'pst'::text)")) @db.Timestamptz(6)
  email        String?
  password     String?
}
